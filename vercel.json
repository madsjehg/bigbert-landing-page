{
  "framework": null,
  "outputDirectory": "public",
  "functions": {
    "api/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "regions": ["iad1"],
  "installCommand": "npm install --legacy-peer-deps",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ]
}
